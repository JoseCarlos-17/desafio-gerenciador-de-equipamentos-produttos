/th>\n          <th>{{ equipment.equipment_type }}</th>\n          <th>{{ equipment.code }}</th>\n          <!-- <th><img :src=\"equipment.equipment_photo\" width=\"100\"></th> -->\n          <th>\n            <v-btn color=\"primary\" size=\"small\">Edit</v-btn>\n            <v-btn color=\"error\" size=\"small\"\n                   @click=\"removeEquipment(equipment.id)\">\n              Remove\n            </v-btn>\n          </th>\n        </tr>\n      </tbody>\n    </v-table>\n  </div>\n</template>\n\n<script>\nimport api_instance from '@/main';\n\nexport default {\n  data() {\n      return {\n        equipmentList: '',\n        // tableLabels: ['Name', 'Brand', 'Local', 'Type', 'Code', 'Photo', 'Actions']\n        tableLabels: ['Name', 'Brand', 'Local', 'Type', 'Code', 'Actions']\n      }\n    },\n\n    methods: {\n      loadEquipment () {\n        api_instance.get('/equipment').then(response => {\n          this.equipmentList = response.data\n        })\n      },\n\n      removeEquipment (equipment) {\n        api_instance.delete(`/equipment/${equipment.id}`).then(response => response)\n      }\n    },\n\n    mounted () {\n      this.loadEquipment()\n    }\n}\n</script>\n\n<style>\n  ul {\n    list-style: none;\n  }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}