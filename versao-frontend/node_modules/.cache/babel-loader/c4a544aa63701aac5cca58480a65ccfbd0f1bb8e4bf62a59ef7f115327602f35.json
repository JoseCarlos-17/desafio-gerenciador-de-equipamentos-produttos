    this.equipment.equipment_photo = this.$refs.file.files[0];\n    },\n    editEquipment() {\n      api_instance.put(`/equipment/${this.$route.params.id}`, {\n        equipment: this.equipment\n      }).then(() => this.$router.push('/'));\n    }\n  },\n  computed: {\n    changeButtonName() {\n      return this.$route.params.id ? 'Edit Equipment' : 'Create Equipment';\n    }\n  },\n  mounted() {\n    this.loadEquipmentOptions();\n    this.loadLocationsOptions();\n    this.loadEditForm();\n  }\n};","map":{"version":3,"mappings":";AAmDE,OAAOA,YAAW,MAAO,QAAQ;AAEnC,eAAe;EACbC,IAAI,GAAG;IACH,OAAO;MACLC,gBAAgB,EAAE,EAAE;MACpBC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,cAAc,EAAE,EAAE;QAClBC,eAAe,EAAE,EAAE;QACnBC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE;MACZ;IACF;EACF,CAAC;EAEHC,OAAO,EAAE;IACLC,oBAAmB,GAAK;MACtBb,YAAY,CAACc,GAAG,CAAC,mCAAmC,EACvCC,IAAI,CAACC,QAAO,IAAK;QAC5B,IAAI,CAACd,gBAAe,GAAIc,QAAQ,CAACf,IAAI,CAACgB,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,GAAG;MAC5D,CAAC;IACH,CAAC;IAEDC,oBAAmB,GAAK;MACtBpB,YAAY,CAACc,GAAG,CAAC,YAAY,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAC9C,IAAI,CAACb,YAAW,GAAIa,QAAQ,CAACf,IAAG;MAClC,CAAC;IACH,CAAC;IAEDoB,eAAc,GAAK;MACjBrB,YAAY,CAACsB,IAAI,CAAC,YAAY,EAAE;QAAElB,SAAS,EAAE,IAAI,CAACA;MAAU,CAAC,EAAE;QAC7DmB,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACF,CAAC,CAAC,CAACR,IAAI,CAAC,MAAM,IAAI,CAACS,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;IACtC,CAAC;IAEDC,YAAW,GAAK;MACd,IAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,EAAE;QACzB,IAAIN,OAAM,GAAI;UAAEA,OAAO,EAAE;YAAE,cAAc,EAAE;UAAsB;QAAE;QAEnEvB,YAAY,CAACc,GAAG,CAAE,cAAa,IAAI,CAACa,MAAM,CAACC,MAAM,CAACC,EAAG,EAAC,EAAEN,OAAO,CAAC,CAACR,IAAI,CAACC,QAAO,IAAK;UAC9E,IAAIf,IAAG,GAAI,IAAI6B,QAAQ,EAAC;UAExB,IAAI,CAAC1B,SAAQ,GAAI;YACfC,IAAI,EAAEW,QAAQ,CAACf,IAAI,CAACI,IAAI;YACxBC,KAAK,EAAEU,QAAQ,CAACf,IAAI,CAACK,KAAK;YAC1BC,cAAc,EAAES,QAAQ,CAACf,IAAI,CAACM,cAAc;YAC5CC,eAAe,EAAEP,IAAI,CAAC8B,MAAM,CAAC,iBAAiB,EAAE,IAAI,CAACvB,eAAe,CAAC;YACrEC,IAAI,EAAEO,QAAQ,CAACf,IAAI,CAACQ,IAAI;YACxBC,IAAI,EAAEM